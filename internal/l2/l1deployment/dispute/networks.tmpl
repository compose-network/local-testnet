[networks.{{.NetworkName}}]
name = "{{.NetworkName}}"
rpc_url = "{{.RpcURL}}"
chain_id = {{.ChainID}}
explorer_url = "{{.ExplorerURL}}"
explorer_api_url = "{{.ExplorerAPIURL}}"

# Required deployment configuration (mapped from user-facing config to compose-contracts schema)
guardian = "{{.GuardianAddress}}"
proxy_admin_owner = "{{.OwnerAddress}}"
authorized_proposer = "{{.ProposerAddress}}"
sp1_verifier = "{{.VerifierAddress}}"
aggregation_vkey = "{{.AggregationVkey}}"

# Optional deployment configuration (with defaults)
proof_maturity_delay_seconds = {{.ProofMaturityDelaySeconds}}
dispute_game_finality_delay_seconds = {{.DisputeGameFinalityDelaySeconds}}
dispute_game_init_bond = {{.DisputeGameInitBond}}
